{
    "baseElmer": true,

    "name": "api",
    "portLocal": 45000,
    "secure": false,
    "forceRegistration": true,
    "dependencies": [],

    "security": {
        "basic": {
            "mode": "basic",
            "realm": "api",
            "users": [
                { "u": "user1", "p": "pass1", "meta": "data" },
                { "u": "user2", "p": "pass2", "meta": "data0" }
            ]
        },
        "jwt": {
            "mode": "jwt",
            "admin": [
                { "u": "cn", "p": "secret", "meta": "data1" }
            ],
            "internal": [
                { "u": "cb", "p": "nonsecret", "meta": "data2" }
            ],
            "external": [
                { "u": "ac", "p": "unsecret", "meta": "data3" }
            ]
        }
    },

    "net": "api",
    "hostname": "dev",
    "dockerBaseName": "elmer",
    "port": 3001,
    "uploadLimitMb": 500,
    "dns1": "1.1.1.1",
    "dns2": "1.0.0.1"
}
